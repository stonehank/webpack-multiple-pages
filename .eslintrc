{
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "import/no-dynamic-require": 0,
    "key-spacing": 0,
    "keyword-spacing": 0,
    "no-mixed-spaces-and-tabs": 0,
    "no-new": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-return-assign": 0,
    "no-tabs": 0,
    "prefer-const": 0,
    "promise/param-names": 0,
    "space-infix-ops": 0,
    "camelcase":1,
    "no-unused-vars":1,
    "import/no-unresolved": 0,
    "semi":0,
    "object-curly-spacing": 0,
    "import/newline-after-import":0,
    "no-use-before-define": 0,
    "global-require": 0,
    "guard-for-in": 0,
    "no-restricted-syntax":0
  },
  "env":{
    "jquery": true
  },
  "globals": {
    "anime": true
  }
}
